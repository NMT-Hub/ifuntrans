{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "python",
      "request": "launch",
      "name": "Launch",
      "program": "${input:main}",
      "stopOnEntry": true,
      "env": {
        "PYTHONPATH": "${workspaceRoot}"
      }
    }
  ],
  "inputs": [
    {
      "id": "main",
      "type": "pickString",
      "options": [
        "server.py"
      ],
      "description": "Program to run: ",
      "default": "server.py"
    }
  ]
}
